<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>
	<suppress checks="Javadoc" files=".*[\\/]test[\\/]" />
	<suppress checks="MagicNumber" files=".*[\\/]test[\\/]" />

    <!-- I don't intend to fix these errors, so I'm going to ignore them completely -->
    <suppress checks="VisibilityModifier" files="queue\/Node\.java" />
    <suppress checks="VisibilityModifier" files="symboltable\/LinkedListSymbolTable\.java" />
    <suppress checks="MemberName" files="graph/*" />
    <suppress checks="MethodName" files="graph/*" />
    <suppress checks="ParameterName" files="graph/*" />
</suppressions>